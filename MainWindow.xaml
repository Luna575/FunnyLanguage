<Window x:Class="FunnyLanguage_WPF.MainWindow"
          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:local="clr-namespace:FunnyLanguage_WPF"
          mc:Ignorable="d"
        Title="FunnyLanguage" MinWidth="1200" MinHeight="550">
    <Grid Background="DarkBlue">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="35"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>

        <Button x:Name="backbtn" Grid.Row="0" HorizontalAlignment="Left" Height="20"  Content="Back to List >> " MinWidth="100" Background="DarkBlue"  Click="Button_List" BorderThickness="0" Foreground="AliceBlue" FontFamily="Arial" FontSize="15" FontStyle="Italic" FontWeight="ExtraLight"></Button>

        <Frame x:Name="Main" NavigationUIVisibility="Hidden" HorizontalAlignment="Center" Grid.Row="2" ></Frame>
    </Grid>
</Window>
